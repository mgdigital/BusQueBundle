{
    "name": "mgdigital/busque-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Symfony bundle for BusQue, a command queue and scheduler for PHP7",
    "autoload": {
        "psr-4": {
            "MGDigital\\BusQue\\Bundle\\": "src"
        }
    },
    "require": {
        "php": ">=7.0",
        "mgdigital/busque": "^0.2.3",
        "league/tactician-bundle": "~0.4",
        "snc/redis-bundle": "~1.0|~2.0",
        "symfony/framework-bundle": "^2.7|~3.0",
        "symfony/monolog-bundle": "~2.11"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "phpspec/prophecy": "~1.6",
        "phpunit/phpunit": "@stable",
        "predis/predis": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "config": {
        "bin-dir": "bin"
    }
}
